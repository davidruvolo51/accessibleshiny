    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-loader": "^4.0.2",
    "postcss-modules": "^3.2.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "repository": {
