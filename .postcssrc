{
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      },
      "postcss-modules": {
        "generateScopedName": "[local]"
      },
      "cssnano": true
    }
}